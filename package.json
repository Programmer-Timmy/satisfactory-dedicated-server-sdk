{
  "name": "satisfactory-dedicated-server-sdk",
  "version": "0.1.1-alpha.12",
  "description": "A TypeScript / JavaScript SDK for interacting with the **Satisfactory** dedicated server API.",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "docs:api-temp": "npx typedoc --out tmp/api-temp src --plugin typedoc-plugin-markdown --hideBreadcrumbs --hidePageTitle --gitRevision main --theme markdown --readme none",
    "docs:copy": "cpy \"tmp/api-temp/**/*.md\" docs/docs/api/ --parents",
    "docs:api": "npm run docs:api-temp && npm run docs:copy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/programmer-timmy/satisfactory-dedicated-server-sdk.git"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/node-fetch": "^2.6.12",
    "copyfiles": "^2.4.1",
    "cpy-cli": "^6.0.0",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.11",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "node-fetch": "^2.7.0"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
